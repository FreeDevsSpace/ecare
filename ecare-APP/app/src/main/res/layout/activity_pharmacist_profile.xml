<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:padding="15dp"
    tools:context="com.example.administrator.ecare.Pharmacist_Profile">


    <TextView
        android:id="@+id/tVPharmacist_ID_Pharmacist_Profile"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginTop="71dp"
        android:text="ID:"
        android:textColor="#000"
        android:textSize="15dp" />

    <TextView
        android:id="@+id/tVName_Pharmacist_Profile"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/tVPharmacist_ID_Pharmacist_Profile"
        android:layout_marginTop="22dp"
        android:text="Name:"
        android:textColor="#000"
        android:textSize="15dp" />

    <TextView
        android:id="@+id/tVMobile_Number_Pharmacist_Profile"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/tVName_Pharmacist_Profile"
        android:layout_marginTop="15dp"
        android:text="Mobile Number: "
        android:textSize="15dp"
        android:textColor="#000"/>

    <TextView
        android:id="@+id/tVDOB_Pharmacist_Profile"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignEnd="@+id/tVQualification_Pharmacist_Profile"
        android:layout_below="@+id/tVMobile_Number_Pharmacist_Profile"
        android:layout_marginTop="15dp"
        android:text="Date Of Birth: "
        android:textColor="#000"
        android:textSize="15dp" />

    <TextView
        android:id="@+id/tVQualification_Pharmacist_Profile"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/tVDOB_Pharmacist_Profile"
        android:layout_marginTop="15dp"
        android:text="Qualification:"
        android:textSize="15dp"
        android:textColor="#000"/>

    <TextView
        android:id="@+id/tVAddress_Pharmacist_Profile"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/tVQualification_Pharmacist_Profile"
        android:layout_marginTop="17dp"
        android:text="Address:"
        android:textSize="15dp"
        android:textColor="#000"/>

    <TextView
        android:id="@+id/tVBlood_Group_Pharmacist_Profile"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/tVAddress_Pharmacist_Profile"
        android:layout_marginTop="29dp"
        android:text=" Blood Group:"
        android:textColor="#000"
        android:textSize="15dp" />

    <TextView
        android:id="@+id/tVP_ID_Pharmacist_Profile"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/tVName_Pharmacist_Profile"
        android:layout_marginStart="40dp"
        android:layout_toEndOf="@+id/tVMobile_Number_Pharmacist_Profile"
        android:text="PID"
        android:textColor="#000"
        android:textSize="15dp" />

    <TextView
        android:id="@+id/tVP_Name_Pharmacist_Profile"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@+id/tVName_Pharmacist_Profile"
        android:layout_alignBottom="@+id/tVName_Pharmacist_Profile"
        android:layout_alignLeft="@+id/tVP_ID_Pharmacist_Profile"
        android:layout_alignStart="@+id/tVP_ID_Pharmacist_Profile"
        android:text="PName"
        android:textColor="#000"
        android:textSize="15dp" />

    <TextView
        android:id="@+id/tVP_MobileNumber_Pharmacist_Profile"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@+id/tVMobile_Number_Pharmacist_Profile"
        android:layout_alignBottom="@+id/tVMobile_Number_Pharmacist_Profile"
        android:layout_alignLeft="@+id/tVP_Name_Pharmacist_Profile"
        android:layout_alignStart="@+id/tVP_Name_Pharmacist_Profile"
        android:text="PMobileNumber"
        android:textColor="#000"
        android:textSize="15dp" />

    <TextView
        android:id="@+id/tVP_DateOfBirth_Pharmacist_Profile"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@+id/tVDOB_Pharmacist_Profile"
        android:layout_alignBottom="@+id/tVDOB_Pharmacist_Profile"
        android:layout_alignLeft="@+id/tVP_MobileNumber_Pharmacist_Profile"
        android:layout_alignStart="@+id/tVP_MobileNumber_Pharmacist_Profile"
        android:text="PDateOfBirth"
        android:textColor="#000"
        android:textSize="15dp" />

    <TextView
        android:id="@+id/tVP_Qualification_Pharmacist_Profile"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@+id/tVQualification_Pharmacist_Profile"
        android:layout_alignBottom="@+id/tVQualification_Pharmacist_Profile"
        android:layout_alignLeft="@+id/tVP_DateOfBirth_Pharmacist_Profile"
        android:layout_alignStart="@+id/tVP_DateOfBirth_Pharmacist_Profile"
        android:text="PQualification"
        android:textColor="#000"
        android:textSize="15dp" />

    <TextView
        android:id="@+id/tVP_Address_Pharmacist_Profile"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@+id/tVAddress_Pharmacist_Profile"
        android:layout_alignBottom="@+id/tVAddress_Pharmacist_Profile"
        android:layout_alignLeft="@+id/tVP_Qualification_Pharmacist_Profile"
        android:layout_alignStart="@+id/tVP_Qualification_Pharmacist_Profile"
        android:text="PAddress"
        android:textColor="#000"
        android:textSize="15dp" />

    <TextView
        android:id="@+id/tVP_BloodGroup_Pharmacist_Profile"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@+id/tVBlood_Group_Pharmacist_Profile"
        android:layout_alignBottom="@+id/tVBlood_Group_Pharmacist_Profile"
        android:layout_alignLeft="@+id/tVP_Address_Pharmacist_Profile"
        android:layout_alignStart="@+id/tVP_Address_Pharmacist_Profile"
        android:text="PBloodGroup"
        android:textColor="#000"
        android:textSize="15dp" />




</RelativeLayout>
