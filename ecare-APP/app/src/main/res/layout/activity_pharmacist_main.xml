<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:visibility="visible"
    tools:context="com.example.administrator.ecare.Pharmacist_Main">

    <ImageView
        android:id="@+id/IVProfile_Pharmacist_Main"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/IVHistory_Pharmacist_Main"
        android:layout_alignStart="@+id/IVSearch_Pharmacist_Main"
        android:layout_marginBottom="60dp"
        android:layout_marginTop="30dp"
        app:srcCompat="@drawable/profile_2" />

    <ImageView
        android:id="@+id/IVSearch_Pharmacist_Main"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_alignStart="@+id/IVHistory_Pharmacist_Main"
        android:layout_marginBottom="220dp"
        android:layout_marginTop="190dp"
        app:srcCompat="@drawable/search_1" />

    <ImageView
        android:id="@+id/IVHistory_Pharmacist_Main"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignTop="@+id/IVSearch_Pharmacist_Main"
        android:layout_centerHorizontal="true"
        android:layout_marginBottom="30dp"
        android:layout_marginRight="140dp"
        android:layout_marginTop="190dp"
        app:srcCompat="@drawable/history_1" />

    <TextView
        android:id="@+id/tVLogout_Pharmacist_Main"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentEnd="true"
        android:layout_alignParentTop="true"
        android:text="Logout"
        android:textSize="20dp"
        android:textColor="#000"/>

</RelativeLayout>
